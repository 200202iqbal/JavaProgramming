package ex1;
import java.util.Scanner;

public class BranchRep1 {

	public static void main (String[] args) {

	Scanner sc =  new Scanner (System.in);

	System.out.print("数字を入力してください :");
	int num = sc.nextInt();
	
	if ( num%3 == 0 )
	{
		System.out.println("\n3の倍数です。");
	}else 
	{
		System.out.println("\n3の倍数ではありません。");
	}

	

	}
}
