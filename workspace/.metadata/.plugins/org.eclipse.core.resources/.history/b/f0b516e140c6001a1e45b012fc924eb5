package ex3;

public class exx1 {

	public static void main(String[] args) {
		// TODO 自動生成されたメソッド・スタブ

		int math = 20;
		int lang = 30;
		int sci = 40;
		int eng = 50;
		int soc = 80;
		int gym = 70;
		int sum = math + lang + sci + eng + soc + gym ;
		int ave = sum / 6 ;
		System.out.println("平均点は" + ave + "点です");
	}

}
